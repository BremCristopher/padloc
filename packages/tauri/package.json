    "license": "GPL-3.0",
    "devDependencies": {
        "@iarna/toml": "2.2.5",
        "@tauri-apps/cli": "2.7.1",
        "clean-webpack-plugin": "4.0.0",
        "css-loader": "7.1.2",
        "dotenv": "17.2.1",
        "file-loader": "6.2.0",
        "html-webpack-plugin": "5.6.3",
        "raw-loader": "4.0.2",
        "style-loader": "4.0.0",
        "ts-loader": "9.5.2",
        "ts-node": "10.9.2",
        "typescript": "5.9.2",
        "webpack": "5.101.0",
        "webpack-cli": "6.0.1",
        "webpack-dev-server": "5.2.2"
    },
    "dependencies": {
        "@padloc/app": "4.3.0",
