{
    "name": "@padloc/tauri",
    "version": "4.3.0",
    "description": "Tauri Wrapper for Padloc",
    "main": "index.js",
    "scripts": {
        "tauri": "tauri",
        "tauricon": "tauri icon",
        "build": "NODE_OPTIONS=--openssl-legacy-provider tauri build",
        "build:debug": "NODE_OPTIONS=--openssl-legacy-provider tauri build --debug",
        "dev": "NODE_OPTIONS=--openssl-legacy-provider tauri dev",
        "update-tauri": "cd src-tauri && cargo update",
        "setup-tauri": "cd src-tauri && cargo build",
        "update:config": "node build-tauri-conf.js"
    },
